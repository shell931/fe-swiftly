{
  "compileOnSave": true,
  "compilerOptions": {
    
    "allowSyntheticDefaultImports": true,
   "baseUrl":  "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "skipLibCheck": true,
    "useDefineForClassFields": false,
  "strict":  false,
  "noImplicitAny": false,
  "strictPropertyInitialization": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@angular/*": ["node_modules/@angular/*"],
      "rxjs/*": ["node_modules/rxjs/*"]
    }
  },
